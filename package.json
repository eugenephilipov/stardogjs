{
  "name": "react-template",
  "version": "1.0.14-1",
  "private": true,
  "dependencies": {
    "material-ui": "0.19.4",
    "react": "^16.0.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-centered": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/addon-storyshots": "3.2.12",
    "@storybook/react": "3.2.12",
    "@types/jest": "21.1.2",
    "@types/material-ui": "0.18.3",
    "@types/node": "8.0.34",
    "@types/react": "16.0.10",
    "@types/react-dom": "16.0.1",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__react": "3.0.5",
    "fork-ts-checker-webpack-plugin": "0.2.8",
    "react-app-rewired": "1.3.1",
    "react-scripts": "1.0.14",
    "react-test-renderer": "16.0.0",
    "ts-jest": "21.1.2",
    "ts-loader": "2.3.7",
    "tslint": "5.7.0",
    "tslint-react": "3.2.0",
    "typescript": "2.5.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx}",
      "!src/**/*stories.{tsx}",
      "!src/{index,registerServiceWorker,setupTests,inject-tap-event-plugin,injectEnv}.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.tsx"
    ],
    "transform": {
      "^.+\\.css$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^(?!.*\\.(ts|tsx|js|jsx|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx|js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js|jsx)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|tsx|js|jsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost"
  }
}
